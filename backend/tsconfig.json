{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "preserveConstEnums": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "outDir": "dist",
    },
    "include": ["src/**/*.ts", "swagger.json"],
  }